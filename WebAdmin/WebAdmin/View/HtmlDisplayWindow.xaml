<Window x:Class="WebAdmin.View.HtmlDisplayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterOwner"
        Title="Error" Height="400" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <WebBrowser Grid.Row="0" Name="WebBrowser"></WebBrowser>
        <Button Grid.Row="1" Margin="5" IsDefault="true" Width="75" x:Name="CloseButton" Click="CloseButton_Click">Close</Button>
    </Grid>
</Window>
